{
    "name": "buzzvel/laravel-github-projects",
    "description": "Laravel/PHP - Total projects on github - package",
    "license": "MIT",
    "authors": [
        {
            "name": "Buzzvel",
            "email": "developers@buzzvel.com",
            "homepage": "https://github.com/Buzzvel"
        }
    ],
    "homepage": "https://github.com/buzzvel/laravel-github-projects",
    "keywords": ["Laravel", "LaravelGithubProjects"],
    "require": {
        "illuminate/support": "~7|~8",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Buzzvel\\LaravelGithubProjects\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Buzzvel\\LaravelGithubProjects\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Buzzvel\\LaravelGithubProjects\\LaravelGithubProjectsServiceProvider"
            ],
            "aliases": {
                "LaravelGithubProjects": "Buzzvel\\LaravelGithubProjects\\Facades\\LaravelGithubProjects"
            }
        }
    }
}
